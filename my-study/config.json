{
  "$schema": "https://raw.githubusercontent.com/revisit-studies/study/main/src/parser/StudyConfigSchema.json",
  "studyMetadata": {
    "title": "Combined Study - Demographics & Visualization Literacy",
    "version": "1.0",
    "authors": ["Kosisochukwu Igbokwe"],
    "date": "2026-01-25",
    "description": "A study consisting of a demographic questionnaire and a visualization literacy assessment focused on glucose data.",
    "organizations": ["Interactions Lab", "University of Calgary"]
  },
  "uiConfig": {
    "contactEmail": "kosiso.igbokwe@ucalgary.ca",
    "helpTextPath": "my-study/assets/help.md",
    "logoPath": "revisitAssets/revisitLogoSquare.svg",
    "withProgressBar": true,
    "autoDownloadStudy": false,
    "withSidebar": true,
    "sidebarWidth": 200
  },
  "baseComponents": {
    "glucoseLineChart": {
      "description": "Line chart stimulus for glucose aggregation data ",
      "type": "react-component",
      "path": "my-study/assets/LineChart.tsx",
      "nextButtonLocation": "belowStimulus",
      "instructionLocation": "aboveStimulus",
      "response": []
    },
    "glucoseBarChart": {
      "description": "Bar chart stimulus for glucose aggregation data",
      "type": "react-component",
      "path": "my-study/assets/BarChart.tsx",
      "nextButtonLocation": "belowStimulus",
      "instructionLocation": "aboveStimulus",
      "response": []
    },
    "glucoseChartComparison": {
      "description": "Comparison of all four glucose chart variations",
      "type": "react-component",
      "path": "my-study/assets/ChartComparison.tsx",
      "nextButtonLocation": "belowStimulus",
      "instructionLocation": "aboveStimulus",
      "response": []
    }
  },
  "importedLibraries": ["mini-vlat"],
  "components": {
    "introduction": {
      "type": "markdown",
      "path": "my-study/assets/introduction.md",
      "response": []
    },
        "visualization-consent": {
      "type": "markdown",
      "path": "my-study/assets/consent.md",
      "response": [
        {
              "id": "participant-name",
              "prompt": "Participant name (please print):",
              "location": "belowStimulus",
              "type": "shortText",
              "placeholder": "Your full name",
              "required": true
            },
            {
              "id": "participant-signature",
              "prompt": "Participant signature:",
              "location": "belowStimulus",
              "type": "shortText",
              "required": true
            },
            {
              "id": "userInitials",
              "prompt": "Please type your initials below to confirm consent for the visualization assessment.",
              "location": "belowStimulus",
              "type": "shortText",
              "placeholder": "Your initials",
              "required": true
            },
            {
              "id": "anonymous-preference",
              "prompt": "I wish to remain anonymous:",
              "location": "belowStimulus",
              "type": "radio",
              "options": [
                "Yes",
                "No"
              ],
              "required": true
            },
            {
              "id": "pseudonym-preference",
              "prompt": "I wish to remain anonymous, but you may refer to me by a pseudonym:",
              "location": "belowStimulus",
              "type": "radio",
              "options": [
                "Yes",
                "No"
              ],
              "required": true
            },
            {
              "id": "pseudonym",
              "prompt": "The pseudonym I chose for myself is:",
              "location": "belowStimulus",
              "type": "shortText",
              "required": false
            },
            {
              "id": "quote-with-name",
              "prompt": "You may quote me and use my name:",
              "location": "belowStimulus",
              "type": "radio",
              "options": [
                "Yes",
                "No"
              ],
              "required": true
            },
            {
              "id": "follow-up-contact",
              "prompt": "You may contact me for a follow-up interview:",
              "location": "belowStimulus",
              "type": "radio",
              "options": [
                "Yes",
                "No"
              ],
              "required": true
            },
            {
              "id": "follow-up-email",
              "prompt": "If yes, please provide an email address where we can reach you:",
              "location": "belowStimulus",
              "type": "shortText",
              "required": false
            }
      ]
    },
    "demographics-personal": {
      "type": "questionnaire",
      "response": [
        {
          "id": "gender-identity",
          "prompt": "What is your gender identity?",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Man",
            "Woman",
            "Non-binary",
            "Two Spirit",
            "Prefer not to answer"
          ]
        },
        {
          "id": "age-group",
          "prompt": "What is your age group?",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "18-25",
            "26-35",
            "36-45",
            "46-56",
            "56-65",
            "66-75",
            "76-85",
            "86+"
          ]
        },
        {
          "id": "education-level",
          "prompt": "What is the highest level of education you have attained?",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "High school or equivalent",
            "Vocational/technical school (2 year)",
            "Some university",
            "University graduate (4 year)",
            "Master's degree",
            "Doctoral degree (PhD)",
            "Professional degree (MD, JD, etc)",
            "Other"
          ]
        },
        {
          "id": "education-other",
          "prompt": "If other, please specify:",
          "required": false,
          "type": "shortText",
          "location": "aboveStimulus"
        },
        {
          "id": "diabetes-status",
          "prompt": "Which of the following best describes your current diabetes-related health status?",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "I have been diagnosed as pre-diabetic",
            "I have gestational diabetes",
            "I have Type 1 diabetes",
            "I have Type 2 diabetes",
            "None of the above"
          ]
        },
        {
          "id": "glucose-tracking-methods",
          "prompt": "How do you track your glucose data? (Select all that apply)",
          "required": true,
          "location": "aboveStimulus",
          "type": "checkbox",
          "options": [
            "Continuous Glucose Monitor (CGM)",
            "Blood glucose meter (finger-prick device)",
            "Paper log/notebook",
            "Mobile app",
            "Insulin pump",
            "I do not track my glucose",
            "Other"
          ]
        },
        {
          "id": "glucose-tracking-other",
          "prompt": "If other, please specify:",
          "required": false,
          "type": "shortText",
          "location": "aboveStimulus"
        },
        {
          "id": "glucose-review-frequency",
          "prompt": "How often do you review your glucose data or reports?",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Daily",
            "Several times per week",
            "Weekly",
            "Monthly",
            "Rarely",
            "Never"
          ]
        }
      ]
    },

    "diabetes-verification-test": {
      "type": "questionnaire",
      "instruction": "Diabetes Verification Test",
      "instructionLocation": "aboveStimulus",
      "nextButtonLocation": "belowStimulus",
      "response": [
        {
          "id": "diagnosed-diabetes",
          "prompt": "Have you ever been told by a healthcare professional that you have diabetes or prediabetes?",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Yes",
            "No"
          ]
        },
        {
          "id": "diabetes-type",
          "prompt": "Which best describes you?",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Type 1 diabetes",
            "Type 2 diabetes",
            "Prediabetes",
            "I do not have diabetes or prediabetes"
          ]
        },
        {
          "id": "diagnosis-year",
          "prompt": "In what year were you diagnosed with diabetes or prediabetes? If this does not apply to you, please enter \"N/A\".",
          "required": true,
          "type": "shortText",
          "location": "aboveStimulus"
        },
        {
          "id": "last-a1c",
          "prompt": "What was your last A1c number?",
          "required": true,
          "type": "shortText",
          "location": "aboveStimulus"
        },
        {
          "id": "insulin-use",
          "prompt": "Do you use insulin? If yes, specify the method (e.g., multiple daily injections, insulin pump).",
          "required": true,
          "type": "shortText",
          "location": "aboveStimulus"
        },
        {
          "id": "a1c-timeframe",
          "prompt": "A1C is a measure of your average blood glucose level for the past:",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Day",
            "Week",
            "6-12 weeks",
            "6 months"
          ]
        },
        {
          "id": "infection-effect",
          "prompt": "What effect will an infection most likely have on blood glucose?",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Lowers it",
            "Raises it",
            "Has no effect"
          ]
        },
        {
          "id": "flu-guidance",
          "prompt": "If you are sick with the flu, you should:",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Take less insulin",
            "Drink less liquids",
            "Eat more proteins",
            "Test blood glucose more often"
          ]
        },
        {
          "id": "rapid-acting-low",
          "prompt": "If you have taken rapid-acting insulin, you are most likely to have a low blood glucose reaction in:",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Less than 2 hours",
            "3-5 hours",
            "6-12 hours",
            "More than 13 hours"
          ]
        },
        {
          "id": "low-blood-glucose-action",
          "prompt": "If you are beginning to have a low blood glucose reaction, you should:",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Exercise",
            "Lie down and rest",
            "Drink some juice",
            "Take rapid-acting insulin"
          ]
        },
        {
          "id": "low-glucose-cause",
          "prompt": "A low blood glucose reaction may be caused by:",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Too much insulin",
            "Too little insulin",
            "Too much food",
            "Too little exercise"
          ]
        },
        {
          "id": "missed-breakfast-insulin",
          "prompt": "You realize just before lunch that you forgot to take your insulin at breakfast. What should you do now?",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Skip lunch to lower your blood glucose",
            "Take the insulin that you usually take at breakfast",
            "Take twice as much insulin as you usually take at breakfast",
            "Check your blood glucose level to decide how much insulin to take"
          ]
        },
        {
          "id": "skip-breakfast-effect",
          "prompt": "If you take your morning insulin but skip breakfast, your blood glucose level will usually:",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Increase",
            "Decrease",
            "Remain the same"
          ]
        },
        {
          "id": "high-glucose-cause",
          "prompt": "High blood glucose may be caused by:",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Not enough insulin",
            "Skipping meals",
            "Delaying your snack",
            "Skipping your exercise"
          ]
        },
        {
          "id": "raise-low-glucose",
          "prompt": "Which item is used to quickly raise low blood glucose?",
          "required": true,
          "location": "aboveStimulus",
          "type": "radio",
          "options": [
            "Insulin",
            "Metformin",
            "Juice or glucose tablets",
            "Long-acting insulin"
          ]
        }
      ],
      "correctAnswer": [
        {
          "id": "a1c-timeframe",
          "answer": "6-12 weeks"
        },
        {
          "id": "infection-effect",
          "answer": "Raises it"
        },
        {
          "id": "flu-guidance",
          "answer": "Test blood glucose more often"
        },
        {
          "id": "rapid-acting-low",
          "answer": "Less than 2 hours"
        },
        {
          "id": "low-blood-glucose-action",
          "answer": "Drink some juice"
        },
        {
          "id": "low-glucose-cause",
          "answer": "Too much insulin"
        },
        {
          "id": "missed-breakfast-insulin",
          "answer": "Check your blood glucose level to decide how much insulin to take"
        },
        {
          "id": "skip-breakfast-effect",
          "answer": "Decrease"
        },
        {
          "id": "high-glucose-cause",
          "answer": "Not enough insulin"
        },
        {
          "id": "raise-low-glucose",
          "answer": "Juice or glucose tablets"
        }
      ]
    },


